@{ 
}
<!DOCTYPE html>
<html>
<head>
    <title>@ViewData["Title"]</title>
    <meta name="description" content="@ViewData["Description"]">
    <meta name="keywords" content="@ViewData["Keywords"]">
    <meta charset="utf-8">
    @Html.Raw(ViewData["MetaTags"])
    <meta name="author" content="Petroteks">
    <!--[if IE]><meta http-equiv='X-UA-Compatible' content='IE=edge,chrome=1'><![endif]-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0" />

    <!-- Favicons -->
    <link rel="icon" type="image/png" sizes="16x16" href="~/images/favicon.png">

    <!-- CSS -->
    <link rel="stylesheet" href="../css/bootstrap.min.css">
    <link rel="stylesheet" href="../css/style.css">
    <link rel="stylesheet" href="../css/style-responsive.css">
    <link rel="stylesheet" href="../css/animate.min.css">
    <link rel="stylesheet" href="../css/vertical-rhythm.min.css">
    <link rel="stylesheet" href="../css/owl.carousel.css">
    <link rel="stylesheet" href="../css/magnific-popup.css">
    <link rel="stylesheet" href="../css/rev-slider.css">
    <link rel="stylesheet" href="../rs-plugin/css/settings.css" media="screen" />
    <link href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/css/toastr.min.css" rel="stylesheet" />
    <style>

        .font-alt-type {
            font-family: "Montserrat", sans-serif;
            text-transform: initial;
            letter-spacing: 0.01em !important;
            color: #413e66;
            margin-bottom: 40px;
            font-size: 48px;
            font-weight: 700;
        }

        .color-blue {
            color: #00A0E3;
        }

        .color-navy-blue {
            color: #413e66;
        }
    </style>
</head>
<body class="appear-animate" itemscope="" itemtype="http://schema.org/WebPage">

    <!-- Page Loader -->
    <div class="page-loader">
        <div class="loader">Loading...</div>
    </div>
    <!-- End Page Loader -->
    <!-- Page Wrap -->
    <div class="page" id="top">

        <!-- Home Section -->
        <!-- End Home Section -->
        <!-- Navigation panel -->
        <nav class="main-nav transparent stick-fixed">
            <div class="full-wrapper relative clearfix">
                <!-- Logo ( * your text or image into link tag *) -->
                <div class="nav-logo-wrap local-scroll">
                    <a href="mp-index.html" class="logo">
                        <img src="http://www.petroteks.com/tr/wp-content/uploads/2019/12/logopetroteks-1.png" itemprop="logo" alt="logo" />
                    </a>
                </div>
                <div class="mobile-nav">
                    <i class="fa fa-bars"></i>
                </div>

                <!-- Main Menu -->
                <div class="inner-nav desktop-nav">
                    <ul class="clearlist">

                        <!-- Item With Sub -->
                        <li>
                            <a asp-controller="Home" asp-action="Index" class="mn-has-sub active"   itemprop="url">ANA SAYFA</a>
                        </li>
                        <li itemscope itemtype="http://schema.org/Product">
                            <a href="#" class="mn-has-sub">ÜRÜNLER <i class="fa fa-angle-down"></i></a>

                            <!-- Sub -->
                            <ul class="mn-sub" >
                                @await Component.InvokeAsync("CategoryList",new { website=(Website)ViewBag.ThisWebsite})
                                
                            </ul>
                            <!-- End Sub -->

                        </li>
                        <li>
                            <a asp-controller="Home" asp-action="Hakkimizda" itemprop="url"class="mn-has-sub active">HAKKIMIZDA</a>
                        </li>
                        <li>
                            <a asp-controller="Home" asp-action="GizlilikPolitikasi" itemprop="url" class="mn-has-sub active">GİZLİLİK POLİTİKASI</a>
                        </li>
                        <!-- End Item With Sub -->
                        <!-- Item With Sub -->
                        <!-- End Item With Sub -->
                        <!-- Divider -->
                        <li><a>&nbsp;</a></li>
                        <!-- End Divider -->
                        <!-- Search -->
                    
                        <!-- End Search -->
                        <!-- Cart -->
                        <!-- End Cart -->
                        <!-- Languages -->
                        <li>
                            <a href="#" class="mn-has-sub">TR <i class="fa fa-angle-down"></i></a>

                            <ul class="mn-sub">

                                <li><a href="">ENG</a></li>

                            </ul>

                        </li>
                        <!-- End Languages -->

                    </ul>
                </div>
                <!-- End Main Menu -->


            </div>
        </nav>
        <!-- End Navigation panel -->

        @RenderBody()

        <footer class="small-section bg-gray-lighter footer pb-60">
            <div class="container">

                <!-- Footer Widgets -->
                <div class="row align-left">

                    <!-- Widget -->
                    <!-- End Widget -->
                    <!-- Widget -->
                    <div class="col-sm-6 col-md-3">
                        <div class="widget">

                            <h5 class="widget-title font-alt">SİTE HARİTASI</h5>

                            <div class="widget-body">
                                <ul class="clearlist widget-menu">
                                    <li>
                                        <a href="#" itemprop="url" title="">ANA SAYFA<i class="fa fa-angle-right right"></i></a>
                                    </li>
                                    <li>
                                        <a href="#" itemprop="url"  title="">HAKKIMIZDA <i class="fa fa-angle-right right"></i></a>
                                    </li>
                                    <li>
                                        <a href="#"  itemprop="url"  title="">ÜRÜNLER<i class="fa fa-angle-right right"></i></a>
                                    </li>
                                    <li>
                                        <a href="#" itemprop="url"  title="">GİZLİLİK POLİTİKASI <i class="fa fa-angle-right right"></i></a>
                                    </li>
                                    <li>
                                        <a href="#" itemprop="url"   title="">İLETİŞİM<i class="fa fa-angle-right right"></i></a>
                                    </li>


                                </ul>
                            </div>

                        </div>
                    </div>
                    <!-- End Widget -->
                    <!-- Widget -->
                    <div class="col-sm-6 col-md-3">
                        <div class="widget">

                            <h5 class="widget-title font-alt">TOKAT DEPO </h5>

                            <div class="widget-body"itemprop="address" itemscope itemtype="http://schema.org/PostalAddress">
                                <ul class="clearlist widget-posts">
                                    <li class="clearfix">
                                        <div class="widget-posts-descr">
                                            
                                            <span itemprop="streetAddress">
                                                Amasya – Erzincan Yolu Boğazbaşı Mahallesi</br>
                                                Mevkii No:177/1-1</br>
                                            </span>
                                            
                                            <span>
                                                Niksar / TOKAT
                                            </span>

                                      
                                        </div>
                                    </li>


                                </ul>


                            </div>
                            <!-- End Widget -->
                        </div>
                        <!-- End Widget -->
                    </div>
                    <!-- End Widget -->
                    <!-- Widget -->
                    <div class="col-sm-6 col-md-3">
                        <div class="widget">

                            <h5 class="widget-title font-alt">ANKARA DEPO </h5>

                            <div class="widget-body">
                                <ul class="clearlist widget-posts">
                                    <li class="clearfix">
                                        <div class="widget-posts-descr">
                                            <a href="#" title="">
                                                İvedik OSB 2268 </br>
                                                Cadde No:9</br>Yenimahalle / ANKARA
                                        </div>
                                    </li>


                                </ul>


                            </div>
                            <!-- End Widget -->
                        </div>
                    </div>
                    <!-- End Widget -->
                    <div class="col-sm-6 col-md-3">
                        <div class="widget">

                            <h5 class="widget-title font-alt">İLETİŞİM</h5>

                            <div class="widget-body">
                                <div class="widget-text clearfix">


                                    <div class="contact-item">
                                        <div class="ci-icon">
                                            <i class="fa fa-phone"></i>
                                        </div>
                                        <div class="ci-title font-alt">
                                            TEL
                                        </div>
                                        <div class="ci-text">
                                            <a href="tel:+90 312 426 23 23">+90 312 426 23 23</a>
                                        </div>
                                    </div>
                                    <div class="contact-item">
                                        <div class="ci-icon">
                                            <i class="fa fa-fax"></i>
                                        </div>
                                        <div class="ci-title font-alt">
                                            FAX
                                        </div>
                                        <div class="ci-text">
                                            <a>+90 312 426 23 23</a>
                                        </div>
                                    </div>
                                </div>
                            </div>

                        </div>
                    </div>
                </div>
                <!-- End Footer Widgets -->
                <!-- Divider -->
                <hr class="mt-0 mb-10 mb-xs-10">
                <!-- End Divider -->
                <!-- Footer Logo -->
                <!-- Footer Logo -->
                <div class="local-scroll mb-30 wow fadeInUp" data-wow-duration="1.5s">
                    <a href="#top"><img src="http://www.petroteks.com/tr/wp-content/uploads/2019/12/logopetroteks-1.png" width="178" height="136" alt="" /></a>
                </div>
                <!-- End Footer Logo -->
                <!-- Social Links -->
                <!-- End Social Links -->
                <!-- Footer Text -->
                <!-- End Footer Text -->


            </div>
        </footer>


        <!-- End Foter -->


    </div>
    <!-- End Page Wrap -->
    <!-- JS -->
    <script type="text/javascript" src="../js/jquery-1.11.2.min.js"></script>
    <script type="text/javascript" src="../js/jquery.easing.1.3.js"></script>
    <script type="text/javascript" src="../js/bootstrap.min.js"></script>

    <script type="text/javascript" src="../js/jquery.scrollTo.min.js"></script>
    <script type="text/javascript" src="../js/jquery.localScroll.min.js"></script>
    <script type="text/javascript" src="../js/jquery.viewport.mini.js"></script>
    <script type="text/javascript" src="../js/jquery.countTo.js"></script>
    <script type="text/javascript" src="../js/jquery.appear.js"></script>
    <script type="text/javascript" src="../js/jquery.sticky.js"></script>
    <script type="text/javascript" src="../js/jquery.parallax-1.1.3.js"></script>
    <script type="text/javascript" src="../js/jquery.fitvids.js"></script>
    <script type="text/javascript" src="../js/owl.carousel.min.js"></script>
    <script type="text/javascript" src="../js/isotope.pkgd.min.js"></script>
    <script type="text/javascript" src="../js/imagesloaded.pkgd.min.js"></script>
    <script type="text/javascript" src="../js/jquery.magnific-popup.min.js"></script>
    <!-- Replace test API Key "AIzaSyAZsDkJFLS0b59q7cmW0EprwfcfUA8d9dg" with your own one below
    **** You can get API Key here - https://developers.google.com/maps/documentation/javascript/get-api-key -->
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAZsDkJFLS0b59q7cmW0EprwfcfUA8d9dg"></script>
    <script type="text/javascript" src="../js/gmap3.min.js"></script>
    <script type="text/javascript" src="../js/wow.min.js"></script>
    <script type="text/javascript" src="../js/masonry.pkgd.min.js"></script>
    <script type="text/javascript" src="../js/jquery.simple-text-rotator.min.js"></script>
    <script type="text/javascript" src="../js/all.js"></script>
    <script type="text/javascript" src="../js/contact-form.js"></script>
    <script type="text/javascript" src="../js/jquery.ajaxchimp.min.js"></script>
    <script type="text/javascript" src="../rs-plugin/js/jquery.themepunch.tools.min.js"></script>
    <script type="text/javascript" src="../rs-plugin/js/jquery.themepunch.revolution.min.js"></script>
    <script type="text/javascript" src="../js/rev-slider.js"></script>
    <script type="text/javascript" src="../js/tiltfx.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.js"></script>
    <script>

        toastr.options = {
            "closeButton": false,
            "debug": false,
            "newestOnTop": false,
            "progressBar": false,
            "positionClass": "toast-top-right",
            "preventDuplicates": false,
            "onclick": null,
            "showDuration": "300",
            "hideDuration": "1000",
            "timeOut": "5000",
            "extendedTimeOut": "1000",
            "showEasing": "swing",
            "hideEasing": "linear",
            "showMethod": "fadeIn",
            "hideMethod": "fadeOut"
        }
    </script>
    <!--[if lt IE 10]><script type="text/javascript" src="../js/placeholder.js"></script><![endif]-->
    @RenderSection("Scripts", required: false)
</body>
</html>
